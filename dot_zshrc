# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"

export NVM_COMPLETION=true
export NVM_LAZY_LOAD=true
export NVM_LAZY_LOAD_EXTRA_COMMANDS=('code')

plugins=(git zsh-nvm)

source $ZSH/oh-my-zsh.sh

alias dc='docker compose'
alias ts='npx tsc'
alias fjs='npx prettier --write $(git diff --name-only)'
alias fmd='npx prettier --write --prose-wrap=always --print-width=100 $(git diff --name-only)'

export PATH="$PATH:$HOME/.bin"

export AZ_TENANT_DEV=cec66056-1016-4c62-9a66-c64bb3d8acf6

eval "$(starship init zsh)"
